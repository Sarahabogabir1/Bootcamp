<%= form_for [@my_project, @my_entry] do |f| %>
	<%= f.label :hours %>
	<%= f.text_field :hours %>
<br>
	<%= f.label :minutes %>
	<%= f.text_field :minutes %>
<br>
	<%= f.label :date %>
	<%= f.date_field :date %>
<br>

	<%= f.submit "Save" %>
<% end %>

<h2> Creating a new time entry in <%= @my_project.name %> project </h2>

<%= render "form" %>